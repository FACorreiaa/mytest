{
  "name": "claiming-service",
  "version": "1.0.0",
  "description": "Claiming service App",
  "author": "Cocus team",
  "license": "MIT",
  "keywords": [
    "angular6",
    "angularCLI",
    "typescript",
    "hospitality"
  ],
  "scripts": {
    "ng": "ng",
    "standardize": "prettier **/*.ts --write",
    "start": "npm run standardize && ng serve --port 8081",
    "build": "npm run standardize && ng build",
    "lint": "ng lint",
    "build:prod": "ng build --prod --vendor-chunk",
    "clean": "rimraf ./dist/",
    "prod": "npm run clean && npm run build:prod",
    "analyze": "npm run clean && npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "tx:push": "tx push -s",
    "tx:pull": "tx pull -a",
    "extract": "ngx-translate-extract -i ./src -o ./src/assets/i18n/de.json ./src/assets/i18n/en.json ./src/assets/i18n/pt.json",
    "extractAuth": "ngx-translate-extract -i ./src/app/auth -o ./src/assets/i18n/auth/de.json ./src/assets/i18n/auth/en.json ./src/assets/i18n/auth/pt.json",
    "extractMain": "ngx-translate-extract -i ./src/app/main -o ./src/assets/i18n/main/de.json ./src/assets/i18n/main/en.json ./src/assets/i18n/main/pt.json",
    "extractCoreAuth": "ngx-translate-extract -i ./src/app/core -o ./src/assets/i18n/auth/de.json ./src/assets/i18n/auth/en.json ./src/assets/i18n/auth/pt.json",
    "extractCoreMain": "ngx-translate-extract -i ./src/app/core -o ./src/assets/i18n/main/de.json ./src/assets/i18n/main/en.json ./src/assets/i18n/main/pt.json",
    "extractFeatureDashboardToMain": "ngx-translate-extract -i ./src/app/features/dashboard -o ./src/assets/i18n/main/de.json ./src/assets/i18n/main/en.json ./src/assets/i18n/main/pt.json",
    "extractChilds": "npm run extractAuth & npm run extractMain & npm run extractCoreAuth & npm run extractCoreMain"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.2",
    "@angular/cdk": "^6.4.3",
    "@angular/common": "^6.1.2",
    "@angular/compiler": "^6.1.2",
    "@angular/core": "^6.1.2",
    "@angular/forms": "^6.1.2",
    "@angular/http": "^6.1.2",
    "@angular/material": "^6.4.3",
    "@angular/platform-browser": "^6.1.2",
    "@angular/platform-browser-dynamic": "^6.1.2",
    "@angular/router": "^6.1.2",
    "@fortawesome/angular-fontawesome": "^0.2.0",
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@ng-bootstrap/ng-bootstrap": "^3.1.1",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "bootstrap": "^4.3.1",
    "browser-detect": "^0.2.27",
    "core-js": "^2.5.4",
    "googlemaps": "^1.12.0",
    "hammerjs": "^2.0.8",
    "keycloak-angular": "^4.1.0",
    "ngrx-store-localstorage": "^5.1.0",
    "ngx-permissions": "^6.0.5",
    "rollbar": "^2.5.2",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.3.3",
    "uuid": "^3.1.0",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.26"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 9-11"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "~6.1.3",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/language-service": "^6.1.2",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/jasmine": "~2.8.3",
    "@types/node": "~9.4.0",
    "all-contributors-cli": "^5.2.1",
    "codelyzer": "~4.4.2",
    "express": "^4.16.3",
    "lint-staged": "^7.2.2",
    "ngrx-store-freeze": "^0.2.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "protractor": "^5.3.2",
    "rimraf": "^2.6.2",
    "standard-version": "^4.2.0",
    "ts-node": "~5.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "prettier": {
    "printWidth": 180,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "parser": "typescript"
  }
}
